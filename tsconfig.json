{
  "compilerOptions": {
    "module": "esnext",
    "target": "es2017",
    "sourceMap": true,
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "alwaysStrict": true,
    "strictNullChecks": true,
    "lib": [
      "dom",
      "es5",
      "ES6",
      "dom.Iterable",
      "scripthost",
      "es2015.proxy",
      "es2019"
    ],
    "outDir": "./static/outDir"
  },
  "exclude": [
    "node_modules"
  ]
}